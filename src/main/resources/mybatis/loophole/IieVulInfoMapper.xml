<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.toolkit.assetscan.dao.loophole.IieVulInfoMapper">

    <select id="getIieVulNum" resultType="integer">
		SELECT
            count(1)
		FROM
			iie_vul_info
		WHERE
			1=1
	</select>

</mapper>